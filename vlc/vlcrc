[visual] # Visualizer filter

# Effects list (reťazec / príkaz)
#effect-list=spectrum

# Video width (hodnota (celé číslo))
#effect-width=800

# Video height (hodnota (celé číslo))
#effect-height=500

# FFT window (reťazec / príkaz)
#effect-fft-window=flat

# Kaiser window parameter (hodnota (desatinná))
#effect-kaiser-param=3.000000

# Show 80 bands instead of 20 (Hodnota boolean)
#visual-80-bands=1

# Draw peaks in the analyzer (Hodnota boolean)
#visual-peaks=1

# Enable original graphic spectrum (Hodnota boolean)
#spect-show-original=0

# Draw the base of the bands (Hodnota boolean)
#spect-show-base=1

# Base pixel radius (hodnota (celé číslo))
#spect-radius=42

# Spectral sections (hodnota (celé číslo))
#spect-sections=3

# V-plane color (hodnota (celé číslo))
#spect-color=80

# Draw bands in the spectrometer (Hodnota boolean)
#spect-show-bands=1

# Show 80 bands instead of 20 (Hodnota boolean)
#spect-80-bands=1

